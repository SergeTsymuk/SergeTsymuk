    </section>
    <!--ContactSection-->
    <section class="panel b-green" id="4">
        <article class="panel__wrapper">
            <div class="panel__content">
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            <div class="contact-content">
                                <div class="heading">
                                    <h4>Contact Me</h4>
                                </div>
                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="contat-form">
                                            <form id="contact" action="https://formsubmit.co/serge.tsymuk@gmail.com" method="post">
                                                <fieldset>
                                                    <div style="color:white">
                                                        <p> Your Name</p>
                                                    </div>
                                                    <input name="name" type="text" class="form-control" id="name" placeholder="Your Name" required>
                                                </fieldset>
                                                <fieldset>
                                                    <div style="color:white">
                                                        <p> Your Email</p>
                                                    </div>
                                                    <input name="email" type="email" class="form-control" id="email" placeholder="Email" required>
                                                </fieldset>
                                                <fieldset>
                                                    <div style="color:white">
                                                        <p> Message</p>
                                                    </div>
                                                    <textarea name="message" rows="6" class="form-control" id="message" placeholder="Message" required></textarea>
                                                </fieldset>
                                                <fieldset>
                                                    <button type="submit" id="form-submit" class="btn" onclick="openPopup()">Send Message</button>
                                                    <div class="popup" id="popup">
                                                        <img src="~/img/Green-Tick-PNG-Pic.png" />
                                                        <h2>Thank you</h2>
                                                        <p> Your form was succesfully sent!</p>
                                                        <button type="button" onclick="closePopup()">OK</button>
                                                    </div>
                                                </fieldset>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
